            <OptimizationLevel Value="3"/>
          </Optimizations>
        </CodeGeneration>
        <Linking>
          <Debugging>
            <GenerateDebugInfo Value="False"/>
          </Debugging>
          <LinkSmart Value="True"/>
        </Linking>
        <Other>
          <CustomOptions Value="-dBorland -dVer150 -dDelphi7 -dCompiler6_Up -dPUREPASCAL"/>
        </Other>
      </CompilerOptions>
    </Item3>
  </BuildModes>
