      </CompilerOptions>
    </Item2>
    <Item3 Name="Release">
      <CompilerOptions>
        <Version Value="11"/>
        <Target>
          <Filename Value="tiFileSync-CML"/>
        </Target>
        <SearchPaths>
          <IncludeFiles Value="../Core"/>
          <Libraries Value="../Core"/>
          <OtherUnitFiles Value="../Core"/>
