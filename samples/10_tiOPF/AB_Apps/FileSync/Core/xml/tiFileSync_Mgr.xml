<!-- property Visibility: public -->
<element name="TtiFileSyncMgr.SourceFileNames">
<short>File Names from source </short>
<descr><p><code>procedure TForm1.ActViewFilesExecute(Sender: TObject);
var
  lForm : TFormViewFiles ;
  lFSM  : TtiFileSyncMgr;
begin
  lFSM  := TtiFileSyncMgr.Create;
  try
    Screen.Cursor := crHourGlass ;
    try
      lFSM.SourceFileNames.StartDir := 'C:\temp\dir1';
      lFSM.SourceReader             := cgsDiskFiles;
      lFSM.TargetFileNames.StartDir := 'C:\temp\dir2';
      lFSM.TargetReader             := cgsDiskFiles;

      lFSM.ReadIndexes ;
    finally
      Screen.Cursor := crDefault ;
    end ;
    lForm := TFormViewFiles.Create( nil ) ;
    try
      lForm.FileSyncMgr := lFSM ;
      lForm.ShowModal ;
    finally
      lForm.Free ;
    end ;
  finally
    lFSM.Free;
  end;
end;
</code>
          </p>
        </descr>
